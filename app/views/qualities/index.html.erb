<p id="notice"><%= notice %></p>

<h1>Qualities</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Icon</th>
      <th>Color</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qualities.each do |quality| %>
      <tr>
        <td><%= quality.title %></td>
        <td><%= quality.description %></td>
        <td><%= quality.icon %></td>
        <td><%= quality.color %></td>
        <td><%= link_to 'Show', quality %></td>
        <td><%= link_to 'Edit', edit_quality_path(quality) %></td>
        <td><%= link_to 'Destroy', quality, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quality', new_quality_path %>
